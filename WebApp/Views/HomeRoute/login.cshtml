@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>AdminLTE 3 | Log in</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- icheck bootstrap -->
    <link rel="stylesheet" href="~/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/dist/css/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>
<body class="hold-transition login-page">
    <div class="login-box">
        <div class="login-logo">
            <a href="~/index2.html"><b>Admin</b>LTE</a>
        </div>
        <!-- /.login-logo -->
        <div class="card">
            <div class="card-body login-card-body">
                <p class="login-box-msg">Sign in to start your session</p>

                <form id="form">
                    <div class="input-group mb-3">
                        <input type="email" class="form-control" name="EmailOrUsername" placeholder="Email">
                        <div class="input-group-append">
                            <div class="input-group-text">
                                <span class="fas fa-envelope"></span>
                            </div>
                        </div>
                    </div>
                    <div class="input-group mb-3">
                        <input type="password" class="form-control" name="Password" placeholder="Password">
                        <div class="input-group-append">
                            <div class="input-group-text">
                                <span class="fas fa-lock"></span>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        @*<div class="col-8">
                        <div class="icheck-primary">
                            <input type="checkbox" id="remember">
                            <label for="remember">
                                Remember Me
                            </label>
                        </div>
                    </div>*@
                        <!-- /.col -->
                        <div class="col-4">
                            <button type="submit" id="SingIn" class="btn btn-primary btn-block">Sign In</button>
                        </div>
                        <!-- /.col -->
                    </div>
                </form>

                @*<p class="mb-1">
                <a href="forgot-password.html">I forgot my password</a>
            </p>*@
                <p class="mb-0">
                    <a href="/guestregister" class="text-center">Register a new membership</a>
                </p>
            </div>
            <div class="card card-primary card-outline">
                <div class="card-header">
                    <h5 class="card-title m-0">without login</h5>
                </div>
                <div class="card-body">
                    <a href="/index" class="btn btn-primary">Cardigan goes on...</a>
                </div>
            </div>
            <!-- /.login-card-body -->
        </div>
    </div>
    <script src="~/plugins/jquery/jquery.min.js"></script>
    <script src="~/lib/general/jquery/dist/jquery.js" type="text/javascript"></script>
    <script src="~/lib/general/popper.js/dist/umd/popper.js" type="text/javascript"></script>
    <script src="~/lib/general/bootstrap/dist/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/lib/general/js-cookie/src/js.cookie.js" type="text/javascript"></script>
    <script src="~/lib/general/moment/min/moment.min.js" type="text/javascript"></script>
    <script src="~/lib/general/perfect-scrollbar/dist/perfect-scrollbar.js" type="text/javascript"></script>
    <script src="~/lib/general/sticky-js/dist/sticky.min.js" type="text/javascript"></script>
    <script src="~/lib/general/jquery-form/dist/jquery.form.min.js" type="text/javascript"></script>
    <script src="~/lib/general/sweetalert2/dist/sweetalert2.min.js" type="text/javascript"></script>
    <script src="~/lib/general/js/global/integration/plugins/sweetalert2.init.js" type="text/javascript"></script>

    <script src="~/lib/general/handlebars/dist/handlebars.js" type="text/javascript"></script>
    <script src="~/lib/general/autosize/dist/autosize.js" type="text/javascript"></script>

    <!-- Bootstrap 4 -->
    <script src="~/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/src/utils/jquery-deseriliazeObject/jquery.deserializeObject.js"></script>
    <script src="~/src/utils/jquery-seriliazeObject/jquery.serializeObject.js"></script>
    <!-- /.login-box -->
    <script src="~/src/constants/urls.js"></script>

    <script src="~/views/js/login.js"></script>
    <!-- jQuery -->
    <script src="~/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/dist/js/adminlte.min.js"></script>

    <script src="~/src/utils/jquery-deseriliazeObject/jquery.deserializeObject.js"></script>
    <script src="~/src/utils/jquery-seriliazeObject/jquery.serializeObject.js"></script>
    <script src="~/src/utils/helpers.js"></script>
    <script src="~/src/js/site.js"></script>
    <script src="~/src/utils/selector.js"></script>
    <script src="~/src/js/plugins.init.js"></script>
    <script src="~/src/utils/request.js"></script>
</body>
</html>
