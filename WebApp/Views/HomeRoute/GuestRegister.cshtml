    <!--end::Fonts -->
    <!--begin::Page Vendors Styles(used by this page) -->
    <!--end::Page Vendors Styles -->
    <!--begin::Global Theme Styles(used by all pages) -->
    <!--begin:: Vendor Plugins -->
    <link href="~/lib/general/perfect-scrollbar/css/perfect-scrollbar.css" rel="stylesheet" type="text/css" />
    <link href="~/lib/general/animate.css/animate.css" rel="stylesheet" type="text/css" />
    <link href="~/lib/general/toastr/build/toastr.css" rel="stylesheet" type="text/css" />

    <link href="~/lib/general/plugins/line-awesome/css/line-awesome.css" rel="stylesheet" type="text/css" />
    <link href="~/lib/general/plugins/flaticon/flaticon.css" rel="stylesheet" type="text/css" />
    <link href="~/lib/general/plugins/flaticon2/flaticon.css" rel="stylesheet" type="text/css" />
    <link href="~/lib/general/@@fortawesome/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css" />
    <link href="~/lib/general/sweetalert2/dist/sweetalert2.css" rel="stylesheet" type="text/css" />

    <!--end:: Ve~/ugins -->
    <link href="~/css/style.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/views/css/custom.css" rel="stylesheet" />

    <!--begin:: ~/Plugins for custom pages -->
    <link href="~/lib/custom/plugins/jquery-ui/jquery-ui.min.css" rel="stylesheet" type="text/css" />
    <!--end:: Ve~/ugins for custom pages -->
    <!--end::Glo~/me Styles -->
    <!--begin::L~/kins(used by all pages) -->
    <link href="~/css/skins/header/base/light.css" rel="stylesheet" type="text/css" />
    <link href="~/css/skins/header/menu/light.css" rel="stylesheet" type="text/css" />
    <link href="~/css/skins/brand/dark.css" rel="stylesheet" type="text/css" />
    <link href="~/css/skins/aside/dark.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <div class="card card-info">
        <div class="card-header">
            <h3 class="card-title">Register</h3>
        </div>
        <!-- /.card-header -->
        <!-- form start -->
        <form class="form-horizontal" id="form">
            <div class="card-body">
                <div class="form-group row">
                    <label for="inputEmail3" class="col-sm-2 col-form-label">Full Name</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" name="FullName" placeholder="FullName">
                    </div>
                </div>
                <div class="form-group row">
                    <label for="inputPassword3" class="col-sm-2 col-form-label">User Name</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" name="UserName" placeholder="UserName">
                    </div>
                </div>
                <div class="form-group row">
                    <label for="inputPassword3" class="col-sm-2 col-form-label">E Mail</label>
                    <div class="col-sm-10">
                        <input type="email" class="form-control" name="Email" placeholder="Email">
                    </div>
                </div>
                <div class="form-group row">
                    <label for="inputPassword3" class="col-sm-2 col-form-label">Password</label>
                    <div class="col-sm-10">
                        <input type="password" class="form-control" name="Password" placeholder="Password">
                    </div>
                </div>
                <input type="hidden" name="UserTypeId" value="2" class="form-control">
            </div>
            <!-- /.card-body -->
            <div class="card-footer">
                <button type="submit" class="btn btn-info">Sign in</button>
                @*<button type="submit" class="btn btn-default float-right">Cancel</button>*@
            </div>
            <!-- /.card-footer -->
        </form>
    </div>

    <!-- jQuery -->
    <script src="~/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/dist/js/adminlte.min.js"></script>
    <script src="~/src/constants/urls.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/dist/js/demo.js"></script>
    <script src="~/src/utils/jquery-deseriliazeObject/jquery.deserializeObject.js"></script>
    <script src="~/src/utils/jquery-seriliazeObject/jquery.serializeObject.js"></script>
    <script src="~/src/utils/helpers.js"></script>
    <script src="~/src/js/site.js"></script>
    <script src="~/src/utils/selector.js"></script>
    <script src="~/src/js/plugins.init.js"></script>
    <script src="~/src/utils/request.js"></script>
    <script src="~/views/js/register.js"></script>
    <script src="~/lib/general/sweetalert2/dist/sweetalert2.min.js" type="text/javascript"></script>

</body>

</html>
